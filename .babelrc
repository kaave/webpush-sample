{
    "env": {
        "development": {
            "presets": [
                ["env", { "targets": { "browsers": "last 2 versions" } }]
            ]
        },
        "production": {
            "presets": [
                ["env", {
                    "targets": {
                        "browsers": "last 2 versions",
                        "uglify": true
                    },
                    "modules": false,
                    "useBuiltIns": true
                }]
            ]
        }
    }
}

